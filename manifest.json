{
    "manifest_version": 2,

    "name": "guelphCourser",
    "description": "Changes UI and functionality of webadvisor uoguelph for the better",
    "version": "1.0",
 
    "content_scripts": [
        {
            "matches": ["https://webadvisor.uoguelph.ca/*"],
            "css": ["ui.css"],
            "js": ["jquery-3.1.1.min.js","contentscript.js"]
        }
    ],
    "web_accessible_resources": ["script.js"],
    "permissions": [
        "activeTab"
    ]
}
